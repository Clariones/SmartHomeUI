<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="bgby.skynet.org.smarthomeui.temperaty.RoundSeekBarUnitActivity">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="51dp"
        android:background="@android:color/black">

        <bgby.skynet.org.customviews.roundseekbar.RoundSeekBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/seekBar"
            app:degree="240"
            app:diameter="200dp"
            app:startDegree="150"
            app:minValue="16"
            app:maxValue="35"
            app:valueHighLimit="35"
            app:valueLowLimit="18"
            app:wheel="@drawable/earth"
            app:arrow="@drawable/roundsliderarrow"
            />
    </RelativeLayout>
</RelativeLayout>
